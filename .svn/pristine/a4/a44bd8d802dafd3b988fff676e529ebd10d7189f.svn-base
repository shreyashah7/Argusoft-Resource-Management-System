.meal-plans {
    font-family:Verdana, Arial, Helvetica, sans-serif;
    border:0px;
}
.meal-plans tr {
    border-bottom:0px;
    border-right: 0px;
    background:#e8eaec;
}
.meal-plans tr.alt-row {
    border-bottom:0px ;
    border-right: 0px;
    background:#e8eaec;
}
.meal-plans td {
    padding:5px;
    text-align:left;
    font-weight:normal;
    /*border-right:1px solid #ffc982;*/
    border-bottom:0px;
    border-right: 0px;
    font-size:12px;
    color:#444;

}
.meal-plans th {
    padding:5px;
    border-bottom:0px ;
    border-right: 0px;
    font-weight:bold;
    text-align:right;
    font-size:12px;
    /*border-right:1px solid #ffc982;*/
}
.meal-plans th.header {
    padding:8px 5px;
    text-align:left;
    font-weight:bold;
    background:#e8eaec url(../../images/nav-bg.jpg) ; 
    background-position: center right,top left;
    background-repeat:repeat-x ;
    border-right: 0px;
    /*	border-right:1px solid #ccc;
    */	

    color:white;
}





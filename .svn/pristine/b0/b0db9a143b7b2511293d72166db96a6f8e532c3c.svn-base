package com.argusoft.armms.web.reports.databean;

import java.util.Date;
import java.util.List;
import javax.faces.bean.ManagedBean;
import javax.faces.bean.RequestScoped;

/**
 * Employee performance report for displaying report on jsf page
 * @author shreya
 */
@ManagedBean(name = "employeePerformanceDataBean")
@RequestScoped
public class EmployeePerformanceDataBean {
    
    private Long userId;
    private String userName;
    private Long projectId;
    private List<Long> projectIds;
    private Date startDate;
    private Date endDate;
    private String taskName;
    private Date taskStartDate;
    private Date taskActualEndDate;
    private String taskActualEndDates;
    private String taskStatus;
    private String performance;
    private String projectName;
    private String label;
    private Long value;

    public Long getUserId() {
        return userId;
    }

    public void setUserId(Long userId) {
        this.userId = userId;
    }

    public String getTaskActualEndDates() {
        return taskActualEndDates;
    }

    public void setTaskActualEndDates(String taskActualEndDates) {
        this.taskActualEndDates = taskActualEndDates;
    }

    public String getTaskName() {
        return taskName;
    }

    public String getLabel() {
        return label;
    }

    public void setLabel(String label) {
        this.label = label;
    }

    public Long getValue() {
        return value;
    }

    public void setValue(Long value) {
        this.value = value;
    }

    public String getUserName() {
        return userName;
    }

    public void setUserName(String userName) {
        this.userName = userName;
    }

    public void setTaskName(String taskName) {
        this.taskName = taskName;
    }

    public String getProjectName() {
        return projectName;
    }

    public void setProjectName(String projectName) {
        this.projectName = projectName;
    }

    public Date getTaskStartDate() {
        return taskStartDate;
    }

    public void setTaskStartDate(Date taskStartDate) {
        this.taskStartDate = taskStartDate;
    }

    public Date getTaskActualEndDate() {
        return taskActualEndDate;
    }

    public void setTaskActualEndDate(Date taskActualEndDate) {
        this.taskActualEndDate = taskActualEndDate;
    }

    public String getTaskStatus() {
        return taskStatus;
    }

    public void setTaskStatus(String taskStatus) {
        this.taskStatus = taskStatus;
    }

    public String getPerformance() {
        return performance;
    }

    public void setPerformance(String performance) {
        this.performance = performance;
    }

    public Long getProjectId() {
        return projectId;
    }

    public void setProjectId(Long projectId) {
        this.projectId = projectId;
    }

    public List<Long> getProjectIds() {
        return projectIds;
    }

    public void setProjectIds(List<Long> projectIds) {
        this.projectIds = projectIds;
    }

    public Date getStartDate() {
        return startDate;
    }

    public void setStartDate(Date startDate) {
        this.startDate = startDate;
    }

    public Date getEndDate() {
        return endDate;
    }

    public void setEndDate(Date endDate) {
        this.endDate = endDate;
    }
    
    
}
